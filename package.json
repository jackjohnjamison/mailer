{
  "name": "mailer",
  "version": "1.0.0",
  "nodemonConfig": {
    "watch": [
      "app.js",
      "routes/",
      "views/",
      "bin"
    ],
    "ext": "js, njk, css",
    "events": {
      "restart": "browser-sync reload"
    }
  },
  "scripts": {
    "start": "node ./bin/www",
    "gulp": "gulp",
    "dev": "concurrently \"npm run gulp\" \"nodemon server.js\" \"browser-sync start --proxy=\"http://localhost:3030/\" --files=\"assets/dist/**/*\" --no-inject-changes\" "
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.2",
    "body-parser": "^1.19.0",
    "browser-sync": "^2.26.13",
    "concurrently": "^5.3.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^4.16.4",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel-minify": "^0.5.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-sass": "^4.1.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.7",
    "nunjucks": "^3.2.2"
  }
}
